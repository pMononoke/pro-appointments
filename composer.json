{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "php-amqplib/php-amqplib": "^2.10",
        "ramsey/uuid": "^3.8",
        "ramsey/uuid-doctrine": "^1.5",
        "symfony/console": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.3.*",
        "symfony/monolog-bundle": "^3.4",
        "symfony/orm-pack": "^1.0",
        "symfony/twig-pack": "^1.0",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "4.3.*"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.2",
        "doctrine/doctrine-fixtures-bundle": "^3.2",
        "friendsofphp/php-cs-fixer": "^2.15",
        "phpstan/phpstan": "^0.12.48",
        "prolic/fpp": "dev-master",
        "sensiolabs/security-checker": "^6.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/test-pack": "^1.0",
        "symfony/web-server-bundle": "4.3.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "ProAppointments\\Collaboration\\": "proAppointments/Collaboration/src/",
            "ProAppointments\\IdentityAccess\\": "proAppointments/IdentityAccess/src/",
            "ProAppointments\\Schedule\\": "proAppointments/Schedule/src/",
            "CompostDDD\\": "lib/compost/src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "ProAppointments\\Collaboration\\Tests\\": "proAppointments/Collaboration/tests/",
            "ProAppointments\\IdentityAccess\\Tests\\": "proAppointments/IdentityAccess/tests/",
            "ProAppointments\\Schedule\\Tests\\": "proAppointments/Schedule/tests/",
            "CompostDDD\\Tests\\": "lib/compost/tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.3.*"
        }
    }
}
